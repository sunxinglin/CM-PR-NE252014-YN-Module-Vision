{
  "urls": "http://0.0.0.0:5000",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}",
          "path": "logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    //"AsZeroDbContext": "User Id=postgres;Host=localhost;Database=traces_tests;Password=mt123456;Encoding=UTF8",
    //"AsZeroDbContext": "Server=your-server;Database=traces;User Id=your-user-id;Password=123456;TrustServerCertificate=True",
    "AsZeroDbContext": "Data Source=AsZero.WebApi.db",
    "ExtensionDbContext": "Data Source=AsZero.Extension.db"
  }
}
